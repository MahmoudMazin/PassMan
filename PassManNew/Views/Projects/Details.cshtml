@model PassManNew.Models.ProjectViewModel

@{
    ViewData["Title"] = "Project Details";
}

<div class="block-header">
    <h1> @ViewData["Title"]</h1>
</div>

<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="row clearfix">
               
                <div class="body">
                    <div class="col-sm-4">
                        <div class="form-group ">
                            <label asp-for="Name"></label>
                            <input asp-for="Name" class="form-control" readonly />
                        </div>

                        <div class="form-group ">
                            <label asp-for="Address"></label>
                            <input asp-for="Address" class="form-control" readonly />
                        </div>

                        <div class="form-group ">
                            <label asp-for="RefNo"></label>
                            <input asp-for="RefNo" class="form-control" readonly />
                        </div>

                        <div class="form-group ">
                            <label asp-for="StartDate"></label>
                            <input asp-for="StartDate" class="form-control" readonly />
                        </div>

                        <div class="form-group ">
                            <label asp-for="EndDate"></label>
                            <input asp-for="EndDate" class="form-control" readonly />
                        </div>
                        <div class="form-group ">
                            <label asp-for="ClientManagerName"></label>
                            <input asp-for="ClientManagerName" class="form-control" readonly />
                        </div>
                        <div class="form-group ">
                            <label asp-for="AzardManagerName"></label>
                            <input asp-for="AzardManagerName" class="form-control" readonly />
                        </div>

                        <div class="form-group ">
                            <label asp-for="State"></label>
                            <input asp-for="State" class="form-control " readonly />
                        </div>



                        <div>
                            <a asp-action="Edit" class="btn bg-yellow waves-effect" asp-route-id="@Model.Id">Edit</a> |
                            <a asp-action="Index" class="btn bg-blue waves-effect">Back to List</a>

                        </div>

                    </div>
                </div>
            
            </div>
        </div>
    </div>
</div>

